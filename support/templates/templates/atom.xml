<?xml version='1.0' encoding='UTF-8'?>
<feed xmlns='http://www.w3.org/2005/Atom' xmlns:g='http://base.google.com/ns/1.0'>
  <title>Reasonably Polymorphic</title>
  <updated>{{#meta}}{{last_updated}}{{/meta}}</updated>
  <author><name>Sandy Maguire</name></author>
  <id>https://reasonablypolymorphic.com</id>
  <link rel='self' href='https://reasonablypolymorphic.com/{{path}}'/>

  {{#meta}}
  {{#posts}}
    <entry>
      <title>{{#meta}}{{a_title}}{{/meta}}</title>
      <id>{{path}}</id>
      <link href='https://reasonablypolymorphic.com/{{path}}'/>
      <published>{{#meta}}{{a_datetime}}{{/meta}}</published>
      <updated>{{#meta}}{{a_datetime}}{{/meta}}</updated>

      <content type="html">
        <![CDATA[
        {{{contents}}}
        ]]>
      </content>
    </entry>
  {{/posts}}
  {{/meta}}
</feed>

